<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns">
  <!-- Properties -->
  <key id="name" for="node" attr.name="name" attr.type="string"/>
  <key id="weight" for="edge" attr.name="weight" attr.type="int"/>
  <!-- Graph -->
  <graph id="G" edgedefault="directed">
    <!-- Vertices -->
    <node id="1">
      <data key="name">A</data>
    </node>
    <node id="2">
      <data key="name">B</data>
    </node>
    <node id="3">
      <data key="name">C</data>
    </node>
    <node id="4">
      <data key="name">D</data>
    </node>
    <!-- Edges -->
    <edge id="5" source="1" target="2" label="goes">
      <data key="weight">5</data>
    </edge>
    <edge id="6" source="2" target="3" label="goes" >
      <data key="weight">4</data>
    </edge>
    <edge id="7" source="3" target="4" label="goes">
      <data key="weight">5</data>
    </edge>
    <edge id="8" source="2" target="4" label="goes">
      <data key="weight">11</data>
    </edge>
    <edge id="9" source="1" target="3" label="goes">
      <data key="weight">10</data>
    </edge>
  </graph>
</graphml>